{
    "plugins":[
        "-lunr", "-search",
        "search-pro@git+https://github.com/cocos-creator/gitbook-plugin-search-pro.git#master",
        "anchorjs",
        "sharing"
    ],
    "pluginsConfig": {
        "search": {
            "maxIndexSize": 1000000000
        },
        "lunr": {
            "maxIndexSize": 1000000000
        },
        "sharing": {
            "douban": false,
            "facebook": true,
            "google": false,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": false,
            "messenger": false,
            "pocket": false,
            "qq": true,
            "qzone": false,
            "stumbleupon": false,
            "twitter": true,
            "viber": false,
            "vk": false,
            "weibo": true,
            "whatsapp": false,
            "all": [
                "facebook", "google", "twitter",
                "weibo", "qzone", "linkedin",
                "pocket", "stumbleupon"
            ]
        }        
    }
}